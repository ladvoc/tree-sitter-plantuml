{
  "name": "plantuml",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "document"
    },
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@startuml"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_command"
          }
        },
        {
          "type": "STRING",
          "value": "@enduml"
        }
      ]
    },
    "_command": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "participant"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "participant": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "participant"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": ".*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

