{
  "name": "plantuml",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "document"
    },
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@startuml"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "STRING",
          "value": "@enduml"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "participant"
        },
        {
          "type": "SYMBOL",
          "name": "skinparam"
        }
      ]
    },
    "_unquoted_string": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "_quoted_string": {
      "type": "PATTERN",
      "value": "\"[\\w\\!\\?\\s]+\""
    },
    "identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_unquoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_unquoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "'.*\\n"
    },
    "keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "as"
        }
      ]
    },
    "skinparam": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "skinparam"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "_participant_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "participant"
        },
        {
          "type": "STRING",
          "value": "actor"
        },
        {
          "type": "STRING",
          "value": "boundary"
        },
        {
          "type": "STRING",
          "value": "control"
        },
        {
          "type": "STRING",
          "value": "entity"
        },
        {
          "type": "STRING",
          "value": "database"
        },
        {
          "type": "STRING",
          "value": "collections"
        },
        {
          "type": "STRING",
          "value": "queue"
        }
      ]
    },
    "participant": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_participant_keyword"
          },
          "named": true,
          "value": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "keyword"
                },
                {
                  "type": "SYMBOL",
                  "name": "string"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

